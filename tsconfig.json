{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "typedocOptions": {
    "mode": "modules",
    "out": "docs",
    "exclude": [ "src/__*__/*", "src/index.ts" ],
    "excludePrivate": true,
    "excludeProtected": true
  }
}
