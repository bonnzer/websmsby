{
    "extends":"airbnb-base",
    "plugins":[
        "import"
    ],
    "parser":"babel-eslint",
    "rules":{
        "indent":["error", 4]
    },
    "parserOptions":{
        "sourceType":"module",
        "ecmaVersion":6,
        "ecmaFeatures":{
            "experimentalObjectRestSpread":true
        }
    },
    "rules":{
      "import/extensions":["error", "never", { "mjs": "always" }]
    }
}
